{
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "src",
    "abis"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "yarn typechain && tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "typechain": "typechain --target ../../node_modules/@hovoh/typechain-ethers-multicall --out-dir ./src/generated/ './abis/*.json'"
  },
  "peerDependencies": {
    "@hovoh/ethcall": "^9.9.9",
    "ethers": "^5.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@hovoh/mai-finance-api",
  "author": "HOVOH",
  "module": "dist/qidaoapi.esm.js",
  "size-limit": [
    {
      "path": "dist/qidaoapi.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/qidaoapi.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@hovoh/ethcall": "^9.9.9",
    "@hovoh/typechain-ethers-multicall": "^10.1.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@typechain/ethers-v5": "^10.0.0",
    "ethers": "^5.7.2",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^8.1.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@hovoh/evmcontractsregistry": "^0.3.3",
    "np": "^7.6.2"
  }
}
